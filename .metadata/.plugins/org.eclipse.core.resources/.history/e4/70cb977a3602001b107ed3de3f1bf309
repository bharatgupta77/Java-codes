package com.hsbc.factorial;

public class Factorial1 {
	
	public Factorial1() {
		
	}
	
	public static int display(int k) {
		int i,fact=1;  
		  //It is the number to calculate factorial    
		  for(i=1;i<=k;i++){    
		      fact=fact*i;    
		  }    
		
		return fact;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		

	}

}
